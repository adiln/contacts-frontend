{{#if currentUser.sessionValid}}
<div class="container-fluid">
    {{#each flashMessages.queue as |flash|}}
      {{flash-message flash=flash}}
    {{/each}}
    <div class="row">
      <div class="col-sm-3" style="background-color:#F1F1F1;">
        {{group-component}}
      </div>
      <div class="col-sm-9" style="background-color:#FFFBEA;">
        {{contact-component}}
      </div>
    </div>
  </div>
{{/if}}

